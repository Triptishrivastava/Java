class Student{
    int Rollno;
    String name;
    int fee;
    public void mymethod(){
        System.out.println("my Rollno:"+Rollno);
        System.out.println("my name:"+name);
    }
    public int totalfee(){
        return fee;
    }
}
public class Custom_Class {
    public static void main(String[] args) {
        System.out.println("This is our custom class");
        Student tripti=new Student();
        tripti.Rollno=44;
        tripti.name="Tripti shrivastava";
        tripti.fee=25000;
        tripti.mymethod();
        System.out.println(tripti.totalfee());
    }
}
